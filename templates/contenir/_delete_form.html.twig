<form method="post" action="{{ path('contenir_delete', {'id': contenir.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ contenir.id) }}">
    <button class="btn btn-sm btn-danger">Supprimer l'article</button>
</form>
